{
  "name": "angular",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "yarn build-search-index",
    "build": "ng build",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "xi18n:all": "concurrently -n 'website,jui,sheet,presets' 'yarn xi18n:website' 'yarn xi18n:jui' 'yarn xi18n:sheet' 'yarn xi18n:presets' && yarn xi18n:combine",
    "xi18n:combine": "ts-node bin/combine-xmb.ts -i 'locale/source/*.xmb' -o locale/source.xmb",
    "xi18n:website": "ng xi18n website --output-path ../../locale/source --out-file website.xmb --i18n-format xmb && ngx-extractor -i 'apps/website/**/*.ts' -f xmb -o locale/source/website.xmb",
    "xi18n:jui": "ng xi18n jui --output-path ../../locale/source --out-file jui.xmb --i18n-format xmb && ngx-extractor -i 'libs/jui/**/*.ts' -f xmb -o locale/source/jui.xmb",
    "xi18n:sheet": "ng xi18n sheet --output-path ../../locale/source --out-file sheet.xmb --i18n-format xmb && ngx-extractor -i 'libs/sheet/**/*.ts' -f xmb -o locale/source/sheet.xmb",
    "xi18n:presets": "ng xi18n presets --output-path ../../locale/source --out-file presets.xmb --i18n-format xmb && ngx-extractor -i 'libs/presets/**/*.ts' -f xmb -o locale/source/presets.xmb",
    "build-search-index": "mkdir -p _generic && ts-node bin/build-search-index.ts > _generic/search-index.ts",
    "gh-pages": "yarn gh-pages:build && yarn gh-pages:prepare && yarn gh-pages:deploy",
    "gh-pages:build": "yarn build website -c de --base-href 'https://draconisnoctis.github.io/dragosia/'",
    "gh-pages:prepare": "cp dist/apps/website/index.html dist/apps/website/404.html",
    "gh-pages:deploy": "ngh --dir dist/apps/website"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.13",
    "@angular/cdk": "~7.3.7",
    "@angular/common": "^7.2.13",
    "@angular/compiler": "^7.2.13",
    "@angular/core": "^7.2.13",
    "@angular/fire": "5.1.2",
    "@angular/forms": "^7.2.13",
    "@angular/material": "7.3.7",
    "@angular/platform-browser": "^7.2.13",
    "@angular/platform-browser-dynamic": "^7.2.13",
    "@angular/router": "^7.2.13",
    "@ngrx/effects": "7.4.0",
    "@ngrx/router-store": "7.4.0",
    "@ngrx/store": "7.4.0",
    "@ngx-translate/i18n-polyfill": "1.0.0",
    "@nrwl/nx": "7.8.0",
    "core-js": "2",
    "firebase": "5.9.4",
    "lunr-languages": "1.1.0",
    "marked": "0.6.2",
    "rxjs": "~6.4.0",
    "tslib": "1.10.0",
    "zone.js": "0.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.8",
    "@angular/cli": "7.3.8",
    "@angular/compiler-cli": "^7.2.13",
    "@angular/language-service": "^7.2.13",
    "@ngrx/entity": "7.4.0",
    "@ngrx/schematics": "7.4.0",
    "@ngrx/store-devtools": "7.4.0",
    "@nrwl/schematics": "7.8.0",
    "@types/fs-extra": "5.0.5",
    "@types/glob": "7.1.1",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/js-yaml": "3.12.1",
    "@types/lunr": "2.3.2",
    "@types/marked": "0.6.5",
    "@types/node": "11.13.4",
    "@types/uuid": "3.4.4",
    "angular-cli-ghpages": "0.5.3",
    "codelyzer": "~5.0.0",
    "concurrently": "4.1.0",
    "fs-extra": "7.0.1",
    "glob": "7.1.3",
    "jasmine-core": "~3.4.0",
    "jasmine-marbles": "0.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "js-yaml": "3.13.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lunr": "2.3.6",
    "ngrx-store-freeze": "0.2.4",
    "ngrx-store-localstorage": "7.0.0",
    "prettier": "1.17.0",
    "protractor": "~5.4.2",
    "ts-node": "8.0.3",
    "tslint": "~5.15.0",
    "typescript": "~3.2.2",
    "uuid": "3.3.2"
  }
}
