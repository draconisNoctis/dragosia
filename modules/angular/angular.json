{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "jui": {
      "root": "libs/jui",
      "sourceRoot": "libs/jui/src",
      "projectType": "library",
      "prefix": "jui",
      "schematics": {
        "@nrwl/schematics:component": {
          "prefix": "jui"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/libs/jui",
            "index": "",
            "main": "libs/jui/src/index.ts",
            "tsConfig": "libs/jui/tsconfig.xi18n.json"
          },
          "configurations": {}
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "jui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/jui/src/test.ts",
            "tsConfig": "libs/jui/tsconfig.spec.json",
            "karmaConfig": "libs/jui/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/jui/tsconfig.lib.json",
              "libs/jui/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "sheet": {
      "root": "libs/sheet",
      "sourceRoot": "libs/sheet/src",
      "projectType": "library",
      "prefix": "js",
      "schematics": {
        "@nrwl/schematics:component": {
          "prefix": "js"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/libs/sheet",
            "index": "",
            "main": "libs/sheet/src/index.ts",
            "tsConfig": "libs/sheet/tsconfig.xi18n.json"
          },
          "configurations": {}
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sheet:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/sheet/src/test.ts",
            "tsConfig": "libs/sheet/tsconfig.spec.json",
            "karmaConfig": "libs/sheet/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/sheet/tsconfig.lib.json",
              "libs/sheet/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "presets": {
      "root": "libs/presets",
      "sourceRoot": "libs/presets/src",
      "projectType": "library",
      "prefix": "jina-draicana",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/libs/presets",
            "index": "",
            "main": "libs/presets/src/index.ts",
            "tsConfig": "libs/presets/tsconfig.xi18n.json"
          },
          "configurations": {}
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "presets:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/presets/src/test.ts",
            "tsConfig": "libs/presets/tsconfig.spec.json",
            "karmaConfig": "libs/presets/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/presets/tsconfig.lib.json",
              "libs/presets/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "website": {
      "root": "apps/website/",
      "sourceRoot": "apps/website/src",
      "projectType": "application",
      "prefix": "dw",
      "schematics": {
        "@nrwl/schematics:component": {
          "prefix": "dw",
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/website",
            "index": "apps/website/src/index.html",
            "main": "apps/website/src/main.ts",
            "polyfills": "apps/website/src/polyfills.ts",
            "tsConfig": "apps/website/tsconfig.app.json",
            "assets": [
              "apps/website/src/favicon.ico",
              "apps/website/src/assets",
              {
                "glob": "{rules,hintergrund,abenteuer}/**/*.md",
                "input": "../",
                "output": "./assets"
              }
            ],
            "styles": [
              "apps/website/src/styles.scss"
            ],
            "scripts": [
              "node_modules/marked/lib/marked.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/website/src/environments/environment.ts",
                  "with": "apps/website/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "de": {
              "fileReplacements": [
                {
                  "replace": "apps/website/src/environments/environment.ts",
                  "with": "apps/website/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "i18nFile": "locale/de.xtb",
              "i18nFormat": "xtb",
              "i18nLocale": "de",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "website:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "website:build:production"
            },
            "de": {
              "browserTarget": "website:build:de"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "website:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/website/src/test.ts",
            "polyfills": "apps/website/src/polyfills.ts",
            "tsConfig": "apps/website/tsconfig.spec.json",
            "karmaConfig": "apps/website/karma.conf.js",
            "styles": [
              "apps/website/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "apps/website/src/favicon.ico",
              "apps/website/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/website/tsconfig.app.json",
              "apps/website/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "website-e2e": {
      "root": "apps/website-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/website-e2e/protractor.conf.js",
            "devServerTarget": "website:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "website:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/website-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "schematics": {
    "@nrwl/schematics:component": {
      "styleext": "scss",
      "viewEncapsulation": "None",
      "changeDetection": "OnPush",
      "prefix": "jd"
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics",
    "packageManager": "yarn"
  },
  "defaultProject": "website"
}
